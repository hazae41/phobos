{
  "type": "module",
  "name": "@hazae41/phobos",
  "description": "Modern and minimalist testing library",
  "version": "2.0.9",
  "repository": "github:hazae41/phobos",
  "author": "hazae41",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "prepare": "rm -rf ./out && tsc && tscousin",
    "version": "deno -RW ./\\$vsync.ts && git add deno.json",
    "synchro": "deno -RW ./\\$dsync.ts && deno install"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "devDependencies": {
    "@hazae41/tscousin": "^1.0.18",
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2"
  }
}